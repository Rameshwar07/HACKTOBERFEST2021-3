#include <iostream>
using namespace std;

int main()
{
    int marks;
    cout << "Enter the marks : - " << endl;
    cin >> marks;
    if (marks >= 70)
    {
        cout << "A-grade..." << endl;
    }
    else if (marks >= 60)
    {
        cout << "B-grade..." << endl;
    }
    else if (marks >= 50)
    {
        cout << "C-grade..." << endl;
    }
    else if (marks >= 40)
    {
        cout << "D-grade..." << endl;
    }
    else
    {
        cout << "fail..." << endl;
    }
    return 0;
}
